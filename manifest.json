{
  "name": "YakShave",
  "version": "1.2",
  "description":
    "Yet Another Key-binding System Halfway Approximating Vi or Emacs",
  "icons": {
    "32": "keyboard-32.png",
    "48": "keyboard-shortcuts-48.png",
    "128": "keyboard-shortcuts-128.png"
  },
  "options_page": "options.html",
  "background_page": "background.html",
  "content_scripts": [
    {
      "js": [ "yakshave.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
